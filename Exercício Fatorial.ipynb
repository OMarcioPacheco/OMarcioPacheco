   ],
   "source": [
    "# leia o valor de n\n",
    "n = int(input(\"Digite um número inteiro não-negativo: \"))\n",
    "\n",
    "# inicializações\n",
    "i = 1  # contador\n",
    "n_fat = 1  \n",
    "\n",
    "# calcule n!\n",
    "while i <= n:\n",
    "    n_fat = n_fat * i \n",
    "    i = i + 1\n",
    "\n",
    "print(\"%d = %d\" %(n, n_fat)) "
   ]
